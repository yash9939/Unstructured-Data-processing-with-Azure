{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "futurepipedf"
		},
		"futurepipecosmsodb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'futurepipecosmsodb'"
		},
		"futurepipedocai_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'futurepipedocai'"
		},
		"futurepipeblobstorage_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://futurestorage.blob.core.windows.net/"
		},
		"futurepipedocai_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://pipeai.cognitiveservices.azure.com/"
		},
		"futurepipedocai_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "future.pipe"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/futurepipleine')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Input_Meta_Data",
						"type": "GetMetadata",
						"dependsOn": [],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"dataset": {
								"referenceName": "futurepipebinary",
								"type": "DatasetReference",
								"parameters": {}
							},
							"fieldList": [
								"childItems"
							],
							"storeSettings": {
								"type": "AzureBlobStorageReadSettings",
								"recursive": true,
								"enablePartitionDiscovery": false
							},
							"formatSettings": {
								"type": "BinaryReadSettings"
							}
						}
					},
					{
						"name": "Doc_Analyze",
						"type": "WebActivity",
						"state": "Inactive",
						"onInactiveMarkAs": "Succeeded",
						"dependsOn": [],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"method": "POST",
							"headers": {},
							"url": "https://pipeai.cognitiveservices.azure.com/formrecognizer/documentModels/prebuilt-read:analyze?api-version=2023-07-31",
							"body": {
								"value": "@dataset().sasurl",
								"type": "Expression"
							},
							"authentication": {
								"type": "Basic",
								"password": {
									"type": "SecureString",
									"value": "**********"
								}
							}
						}
					},
					{
						"name": "Parsing",
						"type": "SetVariable",
						"state": "Inactive",
						"onInactiveMarkAs": "Succeeded",
						"dependsOn": [],
						"policy": {
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"variableName": "",
							"value": ""
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/futurepipebinary')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/futurepipebinary')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "futurepipeblobstorage",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"sasurl": {
						"type": "string",
						"defaultValue": "https://futurestorage.blob.core.windows.net/raw-docs?sp=r&st=2025-11-26T19:46:40Z&se=2025-11-28T04:01:40Z&spr=https&sv=2024-11-04&sr=c&sig=011tgciBjTV85mVTNqD02My8FASydryBCPg1kZSbMXM%3D"
					}
				},
				"annotations": [],
				"type": "Binary",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"container": "raw-docs"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/futurepipeblobstorage')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/futurepipecosmosOutput')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "futurepipecosmsodb",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "CosmosDbSqlApiCollection",
				"schema": {},
				"typeProperties": {
					"collectionName": "documents"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/futurepipecosmsodb')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/futurepipeblobstorage')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"serviceEndpoint": "[parameters('futurepipeblobstorage_properties_typeProperties_serviceEndpoint')]",
					"accountKind": "StorageV2"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/futurepipecosmsodb')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "CosmosDb",
				"typeProperties": {
					"connectionString": "[parameters('futurepipecosmsodb_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/futurepipedocai')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "RestService",
				"typeProperties": {
					"url": "[parameters('futurepipedocai_properties_typeProperties_url')]",
					"enableServerCertificateValidation": true,
					"authenticationType": "Basic",
					"userName": "[parameters('futurepipedocai_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('futurepipedocai_password')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}